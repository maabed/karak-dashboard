.toggleswitch {
  margin-top: 10px;
  margin-bottom: 8px;
  display: inline-block;
}
.toggleswitch .ch-toggle {
  position: absolute;
  visibility: hidden;
}
.toggleswitch .ch-toggle + label {
  display: block;
  position: relative;
  cursor: pointer;
  outline: none;
  user-select: none;
  padding: 2px;
  width: 50px;
  height: 30px;
  background-color: #dddddd;
  border-radius: 60px;
}
.toggleswitch .ch-toggle + label:before {
  right: 1px;
  background-color: #f1f1f1;
  border-radius: 60px;
  bottom: 1px;
  content: "";
  display: inline;
  left: 1px;
  position: absolute;
  top: 1px;
  -webkit-transition: background 0.1s;
  -moz-transition: background 0.1s;
  -o-transition: background 0.1s;
  transition: background 0.1s;
}
.toggleswitch .ch-toggle + label:after {
  width: 30px;
  background-color: #fff;
  border-radius: 100%;
  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.3);
  bottom: 1px;
  content: "";
  display: inline;
  left: 1px;
  position: absolute;
  top: 1px;
  -webkit-transition: margin 0.1s;
  -moz-transition: margin 0.1s;
  -o-transition: margin 0.1s;
  transition: margin 0.1s;
}
.toggleswitch .ch-toggle:checked + label:after {
  margin-left: 20px;
}
.toggleswitch .ch-toggle-green:checked + label:before {
  background-color: #2ecc71;
}
.toggleswitch .ch-toggle-blue:checked + label:before {
  background-color: #3498db;
}
.toggleswitch .ch-toggle-red:checked + label:before {
  background-color: #e74c3c;
}
.toggleswitch .ch-toggle-navy:checked + label:before {
  background-color: #e74c3c;
}
.toggleswitch .ch-toggle-white:checked + label:before {
  background-color: #fff;
}
.checkbox {
  padding-left: 25px;
}
.checkbox input[type="checkbox"] {
  opacity: 0;
}
.checkbox input[type="checkbox"]:checked + label:after {
  font-family: 'FontAwesome';
  content: "\f00c";
}
.checkbox label {
  display: inline-block;
  position: relative;
  padding-left: 0px;
  font-size: 12px;
  font-weight: normal;
  margin-top: 0px;
}
.checkbox label:before {
  content: "";
  display: inline-block;
  position: absolute;
  width: 17px;
  height: 17px;
  left: 0;
  margin-left: -25px;
  border: 1px solid #cccccc;
  border-radius: 3px;
  background-color: #fff;
}
.checkbox label:after {
  display: inline-block;
  position: absolute;
  width: 16px;
  height: 16px;
  margin-left: -25px;
  left: 0;
  top: 0;
  padding-left: 3px;
  padding-top: 1px;
  font-size: 11px;
  color: #555555;
}
.checkbox-primary input[type="checkbox"]:checked + label:before {
  background-color: #67b2e4;
  border-color: #67b2e4;
}
.checkbox-primary input[type="checkbox"]:checked + label:after {
  color: #fff;
}
.checkbox-success input[type="checkbox"]:checked + label:before {
  background-color: #62d995;
  border-color: #62d995;
}
.checkbox-success input[type="checkbox"]:checked + label:after {
  color: #fff;
}
.checkbox-info input[type="checkbox"]:checked + label:before {
  background-color: #99cbed;
  border-color: #99cbed;
}
.checkbox-info input[type="checkbox"]:checked + label:after {
  color: #fff;
}
.checkbox-warning input[type="checkbox"]:checked + label:before {
  background-color: #f5d34b;
  border-color: #f5d34b;
}
.checkbox-warning input[type="checkbox"]:checked + label:after {
  color: #fff;
}
.checkbox-danger input[type="checkbox"]:checked + label:before {
  background-color: #ed796d;
  border-color: #ed796d;
}
.checkbox-danger input[type="checkbox"]:checked + label:after {
  color: #fff;
}
.checkbox-circle label:before {
  border-radius: 50%;
}
